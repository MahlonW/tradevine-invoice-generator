{
  "name": "tradevine-invoice-generator",
  "version": "1.0.0",
  "description": "A modern Svelte-based web application for managing TradeMe sales orders, invoices, and sales analytics",
  "type": "module",
  "keywords": [
    "svelte",
    "trademe",
    "ecommerce",
    "sales",
    "invoices",
    "orders",
    "api",
    "dashboard",
    "analytics"
  ],
  "author": "Mahlon <mahlon.w@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "setup": "node setup-env.js",
    "start": "npm run build && npm run preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
