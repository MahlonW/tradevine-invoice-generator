    "@types/html2canvas": "^0.5.35",
    "@types/node": "^24.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "svelte": "^4.2.0",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "^5.3.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "mysql2": "^3.6.5",
